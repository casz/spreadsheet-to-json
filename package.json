{
  "name": "spreadsheet-to-json",
  "version": "1.2.0",
  "description": "Convert Google Spreadsheets to JSON using Javascript",
  "license": "MIT",
  "repository": "revolunet/spreadsheet-to-json",
  "author": {
    "name": "Julien Bouquillon",
    "email": "julien@bouquillon.com",
    "url": "http://github.com/revolunet"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "start": "babel-node ./index",
    "test": "find ./spec -iname '*.spec.js' -exec babel-node {} \\; | tap-spec",
    "build": "babel -d ./dist ./src"
  },
  "main": "dist/index.js",
  "keywords": [
    "google",
    "spreadsheet",
    "json",
    "drive"
  ],
  "dependencies": {
    "google-spreadsheet": "2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "1.0.0-rc-1",
    "proxyquire": "1.6.0",
    "tap-spec": "4.0.2",
    "tape": "4.0.0"
  }
}
